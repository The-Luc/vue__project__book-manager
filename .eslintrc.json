{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/html-self-closing": "off",
    "vue/max-attributes-per-line": [
      "error",
      {
        "singleline": 5
      }
    ],
    "no-unused-vars": "warn"
  }
}